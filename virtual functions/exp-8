#include <iostream>
using namespace std;

class Animal {
public:
    virtual void speak() = 0;  
};

class Cat : public Animal {
public:
    void speak() override {
        cout << "Meow" << endl;
    }
};

class Dog : public Animal {
public:
    void speak() override {
        cout << "Woof" << endl;
    }
};

int main() {
    Cat c;
    Dog d;

    c.speak();
    d.speak();

    return 0;
}
